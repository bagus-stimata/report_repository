     fproductgroup.`fproductgroupdivisiBean` AS fproductgroup_fproductgroupdivisiBean
FROM
     `ftsalesh` ftsalesh RIGHT OUTER JOIN `ftsalesd` ftsalesd ON ftsalesh.`REFNO` = ftsalesd.`refno`
